# Google Gemini API Configuration
# Obtenha sua chave de API em: https://makersuite.google.com/app/apikey
VITE_GEMINI_API_KEY=

# Configurações opcionais da IA
VITE_DEFAULT_AI_DIFFICULTY=medium
VITE_DEFAULT_AI_LANGUAGE=pt-BR

# Configurações do jogo (opcionais)
VITE_DEFAULT_ROUND_SECONDS=60
VITE_DEFAULT_TARGET_POINTS=10
VITE_SHOW_CARD_BY_DEFAULT=false
VITE_DEFAULT_MAX_POINTS_PER_CARD=5
VITE_DEFAULT_AUTO_PASS_ON_MAX_POINTS=true
VITE_DEFAULT_AI_ONLY_MODE=false
VITE_DEFAULT_THEME=system

# Configuração de desenvolvimento
VITE_ENABLE_DEBUG_LOGS=false
